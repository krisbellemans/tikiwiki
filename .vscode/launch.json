{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Wiki",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/build/src/wiki",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "lldb", // For clang++, use LLDB debugger
      "miDebuggerPath": "/usr/bin/lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
